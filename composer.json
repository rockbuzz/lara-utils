{
  "name": "rockbuzz/lara-utils",
  "description": "Lara Utils",
  "keywords": [
    "utils"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tiago Lopes",
      "email": "tiago@rockbuzz.com.br"
    },
    {
      "name": "Paulo Henrique",
      "email": "paulo@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraUtils\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^7.0",
    "squizlabs/php_codesniffer": "^3.0",
    "laravel/legacy-factories": "^v1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraUtils\\ServiceProvider"
      ]
    }
  }
}